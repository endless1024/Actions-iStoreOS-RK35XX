			rockchip,pins = <4 RK_PA7 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		vcc5v0_usb2_hub_en: vcc5v0-usb2-hub-en {
			rockchip,pins = <4 RK_PA0 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		vcc5v0_host_u3_0_en: vcc5v0-host-u3-0-en {
			rockchip,pins = <4 RK_PB5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		
		vcc5v0_host_u3_1_en: vcc5v0-host-u3-1-en {
			rockchip,pins = <4 RK_PB7 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	usb-typec {
		usbc0_int: usbc0-int {
			rockchip,pins = <4 RK_PC0 RK_FUNC_GPIO &pcfg_pull_up>;
		};

		vcc5v0_tpyec_en: vcc5v0-tpyec-en {
			rockchip,pins = <4 RK_PB6 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};

	headphone {
		hp_det: hp-det {
			rockchip,pins = <3 RK_PD5 RK_FUNC_GPIO &pcfg_pull_none>,
							<4 RK_PD2 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	wireless-bluetooth {
			bt_reset_gpio: bt-reset-gpio {
				rockchip,pins = <2 RK_PB6 RK_FUNC_GPIO &pcfg_pull_none>;
			};

			bt_wake_gpio: bt-wake-gpio {
				rockchip,pins = <1 RK_PD2 RK_FUNC_GPIO &pcfg_pull_none>;
			};

			bt_irq_gpio: bt-irq-gpio {
				rockchip,pins = <0 RK_PA0 RK_FUNC_GPIO &pcfg_pull_none>;
			};
		};

		wireless-wlan {
			wifi_host_wake_irq: wifi-host-wake-irq {
				rockchip,pins = <2 RK_PB3 RK_FUNC_GPIO &pcfg_pull_down>;
			};

			wifi_poweren_gpio: wifi-poweren-gpio {
				rockchip,pins = <2 RK_PB1 RK_FUNC_GPIO &pcfg_pull_up>;
			};
		};
};

